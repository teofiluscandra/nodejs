{
    "env": {
        "browser": true,
        "commonjs": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2015
    },
    "rules": {
        "indent": [2, "tab", { "SwitchCase": 1, "VariableDeclarator": 1 }],
        "no-tabs": 0,
        "no-param-reassign": "off",
        "no-shadow": "off",
        "no-underscore-dangle": "off"
    }
}